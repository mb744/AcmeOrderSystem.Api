{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "DefaultScheme": "LocalAuthIssuer",
    "Schemes": {
      "Bearer": {
        "ValidAudiences":"Local",
        "ValidIssuer": "AcmeWorld",
        "ValidSecretKey": SecretKey
      },
      "LocalAuthIssuer": {
        "ValidAudiences": [
          "https://localhost:8081",
          "http://localhost:8081"
        ],
        "ValidIssuer": "local-auth"
      }
    }
  }
}
